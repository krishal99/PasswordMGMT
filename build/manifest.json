{
  "name": "Password Manager",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Manage your passwords.",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": [
    "background",
    "storage"
  ]
}